            @Html.DisplayFor(modelItem => item.PickupDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PickupWindowStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PickupWindowEnd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeliveryZip)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeliveryWindowStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeliveryWindowEnd)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
